#!/usr/bin/env bash
#bash script that displays number from 1 to 100
#with Fizz Buzz substitued for multiple of 3 and 5

for i in {1..100}
do
    if [ $((i % 5)) -eq 0 -a $((i % 3)) -eq 0 ]
    then
	echo "FizzBuzz"
    elif [ $((i % 5)) -eq 0 ]
    then
	echo "Buzz"
    elif [ $((i % 3)) -eq 0 ]
    then
	echo "Fizz"
    else
	echo "$i"
    fi
    done#!/usr/bin/env bash
